<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anusha.contactsap_">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Contacts" />
        <activity android:name=".Data" />
        <activity android:name=".Images" />
        <activity android:name=".AdapterImages" />
        <activity
            android:name=".ContactDetail"
            android:label="@string/title_activity_contact_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ContactsAdapter" />
    </application>

</manifest>